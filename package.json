{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css --watch",
    "lint:check": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "lint:fix": "npm run lint:check -- --fix",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "format:fix": "prettier --write --ignore-path .gitignore --ignore-unknown .",
    "style:check": "tsc && npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@digitalservice4germany/digital-service-library": "^0.0.10",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "concurrently": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "prettier": "2.5.1",
    "tailwindcss": "^3.0.8",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
